*
    margin 0;
    padding 0;

body
    // 背景图片
    // background-image url('../images/dj.jpg');
    // 背景图片位置固定
    background-attachment fixed;
    // 背景不要重复
    background-repeat no-repeat;
    // 背景位置居中
    background-position center center;
    // 背景覆盖整个viewport
    background-size cover;
    // 当背景没有加载时的颜色
    background-color #b2ae9b;
    font 14px 'SimHei';
    font-weight 300;
.el-input
    color #000000 !important;
.el-input__inner
    background-color: transparent !important;    //绿色
    color: #000000 !important;
.el-table th
    color: #ffffff; /* 字体颜色 */
    font-size: 16px;
    background-color: #15325d !important;/* 背景透明 */
    border: 0;
    height: 30px;
    line-height: 25px;
.el-table__expanded-cell
    background-color: #15325d !important;
.el-table tr, .el-table td
    color: #000000;
    font-size: 14px;
    background-color: #15325d !important;/* 背景透明 */
    border: 0;
    height: 30px;
    line-height: 25px;
.el-table__enable-row-transition .el-table__body td, .el-table .cell
    background-color: #15325d !important;
.el-table::before
    //去除底部白线
    left: 0;
    bottom: 0;
    width: 100%;
    height: 0px;
.el-button
    font-size: 16px;
.el-tabs__item
    color: #ffffff !important

.el-dialog__body
    background #15325d !important
    padding 0px
.el-dialog__header
    background #15325d !important
    padding 0px
.el-dialog__title
    color #FFFFFF !important